select ename, job, sal, deptno from emp where job != "SALESMAN" and deptno != 30; ORRRR <next line>
select ename, job, sal, deptno from emp where (job,deptno) != ("SALESMAN",30);
select DISTINCT(dname), e.deptno from emp e right join dept d on e.deptno = d.deptno where e.deptno IS NOT NUll order by e.deptno;
select ename Employee, sal Monthly_Salary from emp where sal > 1500 and deptno in (10,30);
select ename, sal from emp where sal not BETWEEN 1500 and 2850;
SELECT * from emp where mgr is null;
select ename, job, sal from emp where job in ("MANAGER", "ANALYST") and sal not in (1000,3000,5000);
select ename, sal, COALESCE(comm, 0) commission from emp where (sal+(sal*0.10)) < COALESCE(comm, 0);
select ename from emp where ename like "%LL%" and deptno = 30 or mgr = 7782 ;
select ename, (sal + COALESCE(comm, 0)) Yearly_sal from emp where ename = "SMITH";
select ename, (sal/30) daily_sal from emp where ename = "JONES";
select SUM(sal) total_Monthly_sal from emp ;
select AVG(sal*12) avg_annual_sal from emp;
select ename, sal from emp where (sal%2) != 0;
select * from emp where sal like "___.%";
select * from emp where month(hiredate) = 12;
select * from emp where ename like "%A%";
select * from emp where sal like "%10%" or sal like "%20%" or sal like "%30%";
select * from emp where year(hiredate) IN (select (sal*0.10) from emp); -----No result bcz no data
select concat(lower(substring(ename,1,(length(ename)/2))),upper(substring(ename,length(ename),length(ename)/2))) from emp;--------------Partially correct----------
select * from emp where (year(hiredate)+20) > 1989;
select * from emp order by sal asc;
select * from emp order by deptno asc , job desc;
select DISTINCT(job) from emp order by job desc;
select empno, ename, sal, (sal/30) daily_sal, (sal*12) annual_sal from emp order by (sal*12);
select empno, ename, sal, (1990 - year(hiredate)) Experience from emp where mgr = 7698;
select * from emp where month(hiredate) > 6 and year(hiredate) = 1981 order by job desc;
select ename, (year(CURRENT_DATE) - year(hiredate)) Experience, (sal/30) daily_sal from emp where ((year(CURRENT_DATE) - year(hiredate))+(sal/30)) >100;
select * from emp where job in ("CLERK", "ANALYST") order by job desc;
select * from emp where hiredate IN ("1981-05-01","1981-12-03","1981-12-17","1980-01-19") order by job asc;
select * from emp where year(hiredate) = 1981;
select * from emp where hiredate like "1980-08%";
select ename from emp WHERE length(ename) = 5;
select ename from emp WHERE length(ename) = 5 and ename like "S%";
select ename from emp WHERE length(ename) = 4 and ename like "__R%";
select ename from emp WHERE length(ename) = 5 and ename like "S%H";
select * from emp where month(hiredate) = 1;
select * from emp where MONTHNAME(hiredate) like "_a%" ;
select * from emp where sal like "___0.%";
select * from emp where ename like "%LL%";
select * from emp where year(hiredate) = 1980;
select * from emp where job not in ("PRESIDENT", "MANAGER") and deptno != 20 order by sal asc;
select * from emp where year(hiredate) != 1981 and empno not LIKE "78%";
select * from emp where month(hiredate) != 3;
select * from emp where deptno in (10,30) and year(hiredate) = 1981 ;
select MAX(sal) from emp;
select sum(sal) from emp where job = "MANAGER";
select sum(sal), job from emp where year(hiredate) group by job;
select sum(sal), job from emp where job = "ANALYST";
