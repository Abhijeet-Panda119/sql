select * from emp;
SELECT DISTINCT job from emp;
select * from emp where year(hiredate) < 1981;
SELECT * FROM emp where deptno IN (10,20);
select * from emp where job = "CLERK" and deptno = 20;
select * from emp where ename = "SMITH";
select emp.ename, dept.loc from emp right join dept on emp.deptno = dept.deptno where emp.ename = "SMITH";
select ename , sal from emp where (sal + (sal*0.20)) > 3000;
select ename, job from emp WHERE deptno IN (10,30) and NOT job = "clerk" and hiredate < "1982-12-31";
select * from emp where day(hiredate) < 15;
select * from emp WHERE JOB = "manager";
select ename , concat( '$' , format((sal + (sal*0.15)),2)) as sal_as_dollar from emp;
select concat(ename,'_','and','_',job) as EMP_AND_JOB from emp; 
select concat(ename,'(',job,')') as EMPLOYEE from emp; 
select dname,GROUP_CONCAT(job) jobs from emp right join dept on dept.deptno = emp.deptno group by dname;
select * from emp where ename = 'BLAKE';
select * from emp where job = 'CLERK';
select ename, empno, sal, COALESCE(comm,0) as commision from emp;
select DISTINCT(dname) from dept;
select * from emp where hiredate = "1981-05-01";
select * from emp where job = "MANAGER";
select * from emp where job IN ("CLERK", "MANAGER");
select * from emp where hiredate IN ("1981-05-01", "1981-11-17", "1981-12-30");
select * from emp where year(hiredate) = "1981";
select * from emp where ((sal + COALESCE(comm, 0))*12) BETWEEN 23000 and 40000;
select * from emp where mgr IN (7369,7890,7654,7900);
select * from emp where year(hiredate) = 1982 and month(hiredate) > 06 ;
select ename, sal from emp where ename IN ("MILLER", "SMITH");
SELECT ename, sal from emp where sal >1000;
select ename, sal from emp where ename != "JAMES";
SELECT * from emp where Comm > sal;
select * from emp where ((sal + COALESCE(comm, 0))*12) BETWEEN 22000 and 40000;
SELECT ename, job from emp where mgr IS Null;
SELECT ename from emp where ename like "M%";
SELECT ename from emp where ename like "%H";
select * from emp where year(hiredate) = 1981;
select * from emp where sal like "%00.00";
select * from emp where year(hiredate) like "198%";
SELECT ename, job, hiredate from emp where job = "CLERK" and (1990 - year(hiredate)) > 8;
select ename, job, deptno from emp where job = "MANAGER" and deptno IN (10,20);
select * from emp where ename like "S%";
select ename from emp where ename like "%A%";
SELECT * from emp where ename like "__L%";
select * from emp where ename like "A%" or ename like "M%";